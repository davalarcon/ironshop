<h2>PRODUCT RESULTS</h2>


<ul>
<%  productsAndStuff.forEach((oneProduct)=>{ %>
    <li>
      <h3> <%= oneProduct.name %> </h3>
      <p>$<%=oneProduct.price %> </p>
      <!-- <img src="<%=oneProduct.imageUrl%>" height='200px' alt=""> -->
      <a href="/products/details?myId=<%=oneProduct._id%>">Product Details</a>
      <br>
      <a href="/products/edit?myId=<%=oneProduct._id%>">Edit Details</a>
      <br>

      <a href="/products/delete?myId=<%=oneProduct._id%>">Delete Link</a>
      <br>
      <form action="/products/delete?myId=<%=oneProduct._id%>" method="post">
        <button>Delete Button (post)</button>

      </form>
    </li>
<%  }); %>
</ul>
