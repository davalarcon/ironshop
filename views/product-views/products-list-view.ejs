<h2>PRODUCT RESULTS</h2>


<ul>
<%  productsAndStuff.forEach((oneProduct)=>{ %>
    <li>
      <h3> <%= oneProduct.name %> </h3>
      <p>$<%=oneProduct.price %> </p>
      <!-- <img src="<%=oneProduct.imageUrl%>" height='200px' alt=""> -->
      <a href="/products/<%=oneProduct._id%>/details">Product Details</a>
      <br>
      <a href="/products/<%=oneProduct._id%>/edit">Edit Details</a>
      <br>

      <!-- <a href="/products/<%=oneProduct._id%>/delete">Delete Link</a> -->
      <br>
      <form action="/products/<%=oneProduct._id%>/delete" method="post">
        <button>Delete Button (post)</button>

      </form>
    </li>
<%  }); %>
</ul>
